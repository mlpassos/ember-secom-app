{{yield}}
<div class="row">
	<div class="col s12 m12">
		{{user.userId}}<br>
		{{user.displayName}}<br>
		{{user.email}}
	</div>
	<div class="col s12 m12">
		{{md-switches
	    selection=checkboxSelections
	    content=tags
	    optionValuePath='id'
	    optionLabelPath='title'}}
	</div>
	<div class="col s12 m12">
		selections: {{checkboxSelections}}
		<a href="#" {{action "getPrefs" checkboxSelections}}>Prefs</a>
	</div>
</div>